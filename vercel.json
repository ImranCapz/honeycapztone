{
  "version": 2,
  "builds": [
    {
      "src": "honeypro/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "200mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "ui/staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ui/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "honeypro/wsgi.py"
    }
  ]
}