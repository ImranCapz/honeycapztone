{
  "version": 2,
  "builds": [
    {
      "src": "honeypro/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "200mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "honeypro/staticfiles_build/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "honeypro/staticfiles/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "honeypro/wsgi.py"
    }
  ]
}
